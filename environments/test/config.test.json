{
  "server": {
    "name": "DevOps SRE MCP Server - Test Environment",
    "version": "1.0.0",
    "environment": "test",
    "host": "0.0.0.0",
    "port": 8000,
    "debug": true,
    "workers": 1,
    "max_connections": 100,
    "performance_optimizations": {
      "enabled": false,
      "async_processing": false,
      "connection_pooling": false
    }
  },
  "ollama": {
    "model": "gpt-oss:latest",
    "base_url": "http://localhost:11434",
    "timeout": 60,
    "max_retries": 1,
    "fallback_models": []
  },
  "database": {
    "url": "postgresql://testuser:testpass@localhost:5432/mcptest",
    "pool_size": 5,
    "max_overflow": 10,
    "pool_timeout": 30,
    "echo": true
  },
  "redis": {
    "url": "redis://localhost:6379/1",
    "max_connections": 10
  },
  "cache": {
    "enabled": true,
    "default_ttl": 300,
    "max_size": 100
  },
  "domains": {
    "devops": {"enabled": true},
    "sre": {"enabled": true},
    "cloud": {"enabled": false},
    "platform": {"enabled": false}
  },
  "security": {
    "authentication": {
      "enabled": false
    },
    "rate_limiting": {
      "enabled": false
    }
  },
  "monitoring": {
    "metrics": {"enabled": true},
    "health_checks": {"enabled": true},
    "logging": {
      "level": "DEBUG",
      "format": "text"
    }
  }
}
